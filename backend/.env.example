# Server
SERVER_PORT=8080
SERVER_READ_TIMEOUT=15
SERVER_WRITE_TIMEOUT=15

# Database (Railway provides this automatically as DATABASE_URL)
DATABASE_URL=postgres://paper:paper@localhost:5432/paper?sslmode=disable

# JWT Secrets (generate strong random strings for production!)
JWT_SECRET=change-me-to-a-random-64-char-string
JWT_REFRESH_SECRET=change-me-to-another-random-64-char-string

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# CORS (comma-separated origins)
CORS_ORIGINS=https://your-frontend.vercel.app,http://localhost:5173
